<%= form_tag posts_path do %>
  title: <%= text_field_tag :title %>
  <br>content: <%= text_field_tag :content %>
  <%= submit_tag %>
<% end %>

<hr>
<%= form_tag posts_path do %>
  title: <%= text_field_tag 'post[title]' %>
  <br>content: <%= text_field_tag 'post[content]' %>
  <%= submit_tag %>
<% end %>


<hr>
Use form_for
<br>
<%= form_for @post do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :content %>
  <%= f.submit %>
<% end %>
